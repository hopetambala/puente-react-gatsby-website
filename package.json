{
  "name": "puente-react-gatsby-website",
  "description": "Puente Website Built with Gatsby an Contentful",
  "private": true,
  "version": "0.0.1",
  "author": "Hope Tambala <hope@puente-dr.com>",
  "bugs": {
    "url": "https://github.com/contentful/starter-gatsby-blog/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@stripe/stripe-js": "1.5.0",
    "bootstrap": "4.4.1",
    "env-cmd": "10.1.0",
    "gatsby": "2.24.91",
    "gatsby-image": "2.0.13",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-lodash": "3.3.10",
    "gatsby-plugin-mailchimp": "5.1.2",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-s3": "0.3.3",
    "gatsby-plugin-sass": "2.2.3",
    "gatsby-plugin-sharp": "2.5.6",
    "gatsby-source-contentful": "2.1.78",
    "gatsby-source-stripe": "3.0.7",
    "gatsby-transformer-remark": "2.1.6",
    "gatsby-transformer-sharp": "2.4.6",
    "gh-pages": "2.0.1",
    "lodash": "4.17.13",
    "lottie-web": "5.6.10",
    "node-sass": "4.13.1",
    "react": "16.12.0",
    "react-bootstrap": "1.0.1",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.0",
    "react-icons-kit": "1.3.1",
    "react-player": "2.5.0"
  },
  "keywords": [
    "gatsby",
    "contentful",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/starter-gatsby-blog.git"
  },
  "scripts": {
    "develop": "env-cmd -f .env.development gatsby develop",
    "deploy-dev": "npm run build-dev && gatsby-plugin-s3 deploy -y",
    "deploy": "npm run build-prod && gatsby-plugin-s3 deploy",
    "start": "npm run develop",
    "build-dev": "env-cmd -f .env.development gatsby build",
    "build-prod": "env-cmd -f .env.production gatsby build",
    "heroku-postbuild": "gatsby build",
    "serve": "gatsby serve",
    "reinstall": "gatsby clean && rm -rf package-lock.json && rm -rf node_modules/ && npm install",
    "setup": "npm install --no-save contentful-import yargs-parser axios chalk && node ./setup.js"
  }
}
